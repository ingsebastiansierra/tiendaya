{
  "expo": {
    "name": "TiendaMulti",
    "slug": "tienda-multi",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tiendamulti.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#10B981"
      },
      "package": "com.tiendamulti.app"
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "tiendamulti",
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ]
  }
}
